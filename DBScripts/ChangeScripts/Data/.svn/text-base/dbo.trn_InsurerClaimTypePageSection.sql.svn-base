	/********************************************************************************
	*	Date:	  	Author:		Comments:
	*	09/29/10	EBrosas		Populate records for GARDA - Patient Policy
	**********************************************************************************/

	DECLARE @G int
	SELECT @G = [system_insurer_id] FROM [dbo].[Insurer] WHERE [insurer_code] = 'G' 

	--Delete existing record for Garda - Patient Policy
	IF EXISTS (SELECT system_insurer_id FROM [dbo].trn_InsurerClaimTypePageSection WHERE system_insurer_id = @G AND pageCode = 'CF_Patient')
	BEGIN
		DELETE FROM [dbo].trn_InsurerClaimTypePageSection 
		WHERE system_insurer_id = @G AND pageCode = 'CF_Patient'
	END
	
	IF EXISTS (SELECT system_insurer_id FROM [dbo].trn_InsurerClaimTypePageSection WHERE system_insurer_id = @G AND pageCode = 'CF_Clinical')
	BEGIN
		DELETE FROM [dbo].trn_InsurerClaimTypePageSection 
		WHERE system_insurer_id = @G AND pageCode = 'CF_Clinical'
	END

	INSERT INTO [dbo].[trn_InsurerClaimTypePageSection]([system_insurer_id],[claim_type_code],[pageCode],[sectionPanelId],[sectionName],[createUser],[updateUser], [SortOrder]) 

	SELECT @G,'HCT1','CF_Patient','pnlPolicyDetails','Policy Details','Script','Script',1 UNION
	SELECT @G,'HCT1','CF_Patient','pnlMaternity','Claim Details','Script','Script',2 UNION
	SELECT @G,'HCT1','CF_Patient','pnlInjuryDetails','Third Party Details','Script','Script',3 UNION
	--HOSPITAL CLINICAL CONSULTANT
	SELECT @G,'HCT1','CF_Clinical','pnlHistory','Consultant and Medical Section','Script','Script',1 UNION
	SELECT @G,'HCT1','CF_Clinical','pnlDiagnosis','Investigations','Script','Script',2 UNION
	SELECT @G,'HCT1','CF_Clinical','pnlDischargeStatus','Discharge Status','Script','Script',3 



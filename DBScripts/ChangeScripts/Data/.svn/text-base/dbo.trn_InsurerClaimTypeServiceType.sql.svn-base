DECLARE @N int
SELECT @N = [system_insurer_id] FROM [dbo].[Insurer] WHERE [insurer_code] = 'N'

IF NOT EXISTS (SELECT * FROM [dbo].trn_InsurerClaimTypeServiceType
	WHERE [SystemInsurerId] = @N AND [ClaimTypeCode] = 'HCT1' AND [ServiceTypeId] = 1)
BEGIN
	INSERT INTO [dbo].trn_InsurerClaimTypeServiceType(SystemInsurerId, ClaimTypeCode, ServiceTypeId)
	VALUES(@N, 'HCT1', 1)
END



DECLARE @M int
SELECT @M = [system_insurer_id] FROM [dbo].[Insurer] WHERE [insurer_code] = 'M'

IF NOT EXISTS (SELECT * FROM [dbo].trn_InsurerClaimTypeServiceType
	WHERE [SystemInsurerId] = @M AND [ClaimTypeCode] = 'HCT1' AND [ServiceTypeId] = 5)
BEGIN
	INSERT INTO [dbo].trn_InsurerClaimTypeServiceType(SystemInsurerId, ClaimTypeCode, ServiceTypeId)
	VALUES(@M, 'HCT1', 5)
END
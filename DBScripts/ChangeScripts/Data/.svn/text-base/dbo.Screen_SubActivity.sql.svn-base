/********************************************************************************
*	Revision History
*	Name: Screen_SubActivity
*	Description: 
*
*	Date:	  	Author:		Comments:
*	10/20/2010	JVillas		Renamed sub-activity 'Refer to Debt Collection Agency' to 'Referred Claim Options'
*
*********************************************************************************/

DECLARE @sub_activity_id int

SELECT	@sub_activity_id = sub_activity_id
FROM	dbo.Screen_SubActivity
WHERE	screen_id = '5'
		AND sub_activity_name = 'Refer to Debt Collection Agency'

IF ISNULL(@sub_activity_id, 0) > 0
BEGIN
	UPDATE	dbo.Screen_SubActivity
	SET		sub_activity_name = 'Referred Claim Options'
	WHERE	sub_activity_id = @sub_activity_id
END

SELECT	@sub_activity_id = sub_activity_id
FROM	dbo.Screen_SubActivity
WHERE	screen_id = '5'
		AND sub_activity_name = 'Referred Claim Options'

IF ISNULL(@sub_activity_id, 0) = 0
BEGIN
	INSERT INTO dbo.Screen_SubActivity 
	(
		screen_id
		,sub_activity_name
		,page_control_id
		,isactive
	) 
	VALUES 
	(
		'5'
		,'Referred Claim Options'
		,''
		,1
	)

	SET @sub_activity_id = SCOPE_IDENTITY()
END

IF NOT EXISTS
(
	SELECT	permission 
	FROM	dbo.Profile_SubActivity 
	WHERE	profile_name = 'Administrator' 
			AND sub_activity_id = @sub_activity_id
)
BEGIN
	INSERT INTO dbo.Profile_SubActivity 
	(
		profile_name
		,sub_activity_id
		,permission
	) 
	VALUES 
	(
		'Administrator'
		,@sub_activity_id
		,1
	)
END
ELSE
BEGIN
	UPDATE	dbo.Profile_SubActivity
	SET		permission = 1
	WHERE	profile_name = 'Administrator'
			AND sub_activity_id = @sub_activity_id
END
